{
  "name": "mikedre",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "clean-css": "^3.4.20",
    "concurrently": "3.0.0",
    "node-sass": "^3.10.1",
    "react-scripts": "0.6.1"
  },
  "dependencies": {
    "history": "4.2.0",
    "re-base": "2.2.0",
    "react": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-dom": "15.3.2",
    "react-router": "4.0.0-alpha.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "autoprefixer": "autoprefixer",
    "build-css": "node-sass --include-path scss ./src/css/style.scss ./src/css/style.css",
    "clean": "cleancss -o ./src/css/style-min.css ./src/css/style.css",
    "watch": "concurrently --kill-others \"npm run build-css\" \"npm run clean\""
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
